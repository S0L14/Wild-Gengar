#include <iostream>
using namespace std;

void oldie (){
    cout<<endl;
    cout<<"You're not my type.";
    cout<<endl;
}

void youngin () {
    cout<<endl;
    cout<<"Zamn! She 12? Are you single?";
    cout<<endl;
}

int main () {
    cout<<"Welcome to my program!"<<endl<<"How old are you?"<<endl<<"Enter your age: ";
